---

batch_connect:
  template: "basic"
script:
  queue_name: <%= custom_queue.blank? ? "regular" : custom_queue.strip %>
  native:
    - "-c"
    - "<%= num_cores.blank? ? 1 : num_cores.to_i %>"
    - "--mem"
    - "<%= memory.blank? ? 2000 : memory.to_i * 1000 %>"
    - "-t"
    - "<%= wall_time %>"
